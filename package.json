{
  "name": "ar-pathfinder",
  "version": "0.1.0",
  "description": "Proof of concept Accounts Receiveable application for the Environmental Assessment Office",
  "main": "index.js",
  "dependencies": {
    "async": "^3.1.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.13.4",
    "helmet": "^3.15.0",
    "livescript": "^1.6.0",
    "morgan": "^1.7.0",
    "node-schedule": "^1.3.2",
    "pg": "^7.9.0",
    "pug": "^2.0.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-plugin-vue": "^5.1.0",
    "livescript-plugin-loader": "^2.0.0",
    "material-icons": "^0.3.0",
    "noty": "^3.2.0-beta",
    "object-assign": "^4.1.1",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supervisor": "^0.12.0",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.8",
    "vue-cli-plugin-source-dir": "^0.1.0",
    "vue-livescript-loader": "0.0.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8",
    "vuesax": "^3.10.5",
    "vuex": "^3.1.0"
  },
  "engine": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "dev": "node_modules/supervisor/lib/cli-wrapper.js -x node_modules/livescript/bin/lsc -w backend/ls/server.ls -e ls backend/ls/server.ls & vue-cli-service serve",
    "pack": "vue-cli-service serve",
    "get-token": "node_modules/livescript/bin/lsc backend/ls/get-token.ls",
    "harvest-proponents": "node_modules/livescript/bin/lsc backend/ls/harvest-proponents.ls",
    "start": "node index.js",
    "build": "vue-cli-service build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popkinj/ar-pathfinder"
  },
  "author": "Jamie Popkin <popkinj@littleearth.ca> (https://littleearth.ca)",
  "license": "UNLICENSED",
  "homepage": "http://ar-pathfinder2-bbgloi-dev.pathfinder.gov.bc.ca",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "vue": {
    "outputDir": "frontend/dist",
    "pluginOptions": {
      "sourceDir": "frontend/src"
    },
    "configureWebpack": {
      "devServer": {
        "inline": true,
        "port": 8081
      },
      "module": {
        "rules": [
          {
            "test": "/.vue$/",
            "use": [
              {
                "loader": "livescript-plugin-loader"
              }
            ]
          },
          {
            "test": "/.ls$/",
            "use": [
              {
                "loader": "livescript-plugin-loader"
              }
            ]
          }
        ]
      }
    }
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  }
}
